# -*- Makefile -*-

####################################################################################################

all: book-browser.rcc BookBrowserRessource.py

####################################################################################################

%.rcc : %.qrc
	rcc-qt5 -binary $< -o $@

BookBrowserRessource.py : book-browser.qrc
	pyrcc5 -o $@ $<

####################################################################################################

clean:
	rm *.py *.rcc
